pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                script {
                    // Checkout the code from the specified branch
                    checkout([$class: 'GitSCM', branches: [[name: 'rimMabrouki5SAE6']], userRemoteConfigs: [[url: 'https://github.com/WassimB12/5SAE6_Wamya_SkyStation']]])
                }
            }
        }




        // Add more stages as needed
    }}

